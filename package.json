    "autoprefixer": "10.4.20",
    "chalk": "^5.4.1",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "localtunnel": "^2.0.2",
    "ngrok": "^5.0.0-beta.2",
    "postcss": "8.4.49",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
