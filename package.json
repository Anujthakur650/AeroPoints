    "@react-aria/i18n": "3.12.5",
    "@react-three/drei": "^9.99.0",
    "@react-three/fiber": "^8.15.16",
    "framer-motion": "^11.18.2",
    "framer-motion-3d": "^12.4.13",
    "fuse.js": "^7.1.0",
    "react": "^18.3.1",
