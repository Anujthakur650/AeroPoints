    "deploy:test": "npm run build:quick && npm run preview",
    "rollback:airport-search": "bash scripts/rollback-airport-search.sh",
    "share": "bash share-website.sh",
    "share:stop": "bash stop-sharing.sh",
    "share:status": "node tunnel-status-simple.js",
    "share:monitor": "node tunnel-status.js",
    "share:win": "share-website.bat",
    "share:stop:win": "stop-sharing.bat",
    "share-mobile": "node mobile-share.js",
    "mobile:test": "node mobile-test.js",
    "mobile:qr": "node generate-qr.js"
  },
  "dependencies": {
    "@heroui/react": "2.7.4",
    "@heroui/use-theme": "2.1.6",
